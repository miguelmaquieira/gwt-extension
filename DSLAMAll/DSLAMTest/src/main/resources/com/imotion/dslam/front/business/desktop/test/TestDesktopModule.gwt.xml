	<inherits name="com.imotion.dslam.front.business.desktop.CRONIOBusDesktopModule" />
	
	<replace-with class="com.imotion.dslam.front.business.desktop.test.client.TestDesktopAppEntryPoint">
		<when-type-is class="com.imotion.dslam.front.business.desktop.client.CRONIOBusDesktopAppEntryPoint"/>

snippet switch
	switch (${1}) {
		case ${2}: 
		{
			${3};
			break;
		}
		default: 
		{
			${4};
			break;
		}
	}
	
snippet case 
	case ${1}
	{
		${1};
		break;
	}
	

snippet rbCase
	rbCase "${1}" {
		${2}//TODO:
	}

snippet rbDefault
	rbDefault {
		${1}//TODO:
	}

snippet for
	for ${1} in (${2} .. ${3}) {
		${4}//TODO:
	}

snippet foreach
	foreach (${1} : ${2}) {
		${3}//TODO:
	}

snippet while
	while (${1}) {
		${2}//TODO:
	}

<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>imotionsaboreandotestapp</application>
	<version>1</version>

	<!-- By default, App Engine sends requests serially to a given web server. 
		To allow App Engine to send multiple requests in parallel specify: -->
	<threadsafe>true</threadsafe>

	<sessions-enabled>true</sessions-enabled>

	<!-- Configure serving/caching of GWT files -->
	<static-files>
		<include path="**" />

		<!-- The following line requires App Engine 1.3.2 SDK -->
		<include path="**.nocache.*" expiration="0s" />

		<include path="**.cache.*" expiration="365d" />
		<exclude path="**.gwt.rpc" />
	</static-files>

	<resource-files>
		<exclude path="WEB-INF/deploy/**"></exclude>
		<exclude path="WEB-INF/lib/gwt-dev-2.5.1.jar"></exclude>
		<exclude
			path="com.imotion.saboreando.front.business.desktop.entrypoint.BusinessDesktopModule/**"></exclude>
	</resource-files>

	<system-properties>
		<!-- <property name="java.util.logging.config.file" value="WEB-INF/logging.properties" 
			/> -->
		<property name="file.encoding" value="UTF-8" />
		<property name="DEFAULT_ENCODING" value="UTF-8" />
	</system-properties>

</appengine-web-app>
